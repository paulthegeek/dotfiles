export EDITOR='sublime'
#Sublime Editor
alias sb='subl -n'

#Atom Editor
alias at='atom -n'

#Git
alias g='git'
alias gs='git status'
alias gc='git checkout'
alias gp='git pull'
alias gd='git diff'
alias gpo='git pull origin'

#Utopia
alias ut='utopia'

#Bundler
alias b='bundle'
alias be='bundle exec'
alias bi='bundle install'

#guard
alias gu='bundle exec guard'

#Rails
alias rs='be rails s'
alias rc='be rails c'

#ZSH
alias reload='. ~/.zshrc'

# List Directory Content
alias l='ls -a'

# Open Directories
alias src='cd ~/src'
alias dev='cd ~/src/dev'
