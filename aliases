#Bundler
alias b='bundle'
alias be='bundle exec'
alias bi='bundle install'

#Directories
alias l='ls -a'
alias src='cd ~/src/'
alias dev='cd ~/src/dev/'
alias df='cd ~/src/dotfiles'

#Git
alias g='git'
alias gs='git status'
alias gc='git checkout'
alias gp='git pull'
alias gf='git fetch'
alias gr='git rebase'

#Heroku
alias fs='foreman start'

#Make
alias mk='make'
alias mks='make -s'

#Rails
alias rs='./bin/rails server'
alias rc='./bin/rails console'
alias rk='./bin/rake'

#Shell
alias c='clear'

#VIM
alias v='vim'

#Tmux
alias tm='tmux'

#Valgrind
alias vg='valgrind'

#Zsh
alias reload='. ~/.zshrc'

